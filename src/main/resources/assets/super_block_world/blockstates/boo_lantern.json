{
  "multipart": [
    {
      "when": {
        "lit": "false"
      },
      "apply": {
        "model": "super_block_world:block/boo_lantern_off"
      }
    },
    {
      "when": {
        "lit": "true"
      },
      "apply": {
        "model": "super_block_world:block/boo_lantern_on"
      }
    },
    {
      "when": {
        "hanging": "true"
      },
      "apply": {
        "model": "super_block_world:block/boo_lantern_chain"
      }
    }
  ]
}